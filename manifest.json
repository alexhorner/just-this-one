{
  "name": "Just this one",
  "description": "Shop smarter: skip bundles, buy just what you need",
  "version": "1.0",
  "manifest_version": 3,

  "permissions": ["activeTab"],
  "icons": {"48": "icon48.png", "128": "icon128.png"},
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": [
        "https://*.aliexpress.com/*",
        "https://aliexpress.com/*"
      ]
    }
  ]
}